scope socks
{
    union SocksAddressType switch (uint8)
    {
        case 1: octets[4] ipv4Address;
        case 3: string domainName;
        case 4: octets[16] ipv6Address;
    }

    struct SocksAddress
    {
        socks::SocksAddressType addressType;
        uint16 port;
    }

    scope control
    {
        struct SocksRouteEx
        {
            socks::SocksAddress socksAddress;
        }
    }

    scope stream
    {
        struct HttpBeginEx
        {
            socks::SocksAddress socksAddress;
        }
    }
}
